
#include<iostream>

using namespace std;

#define size 5

int stack[size];

int top=-1;

void push(int val) {

if (top<size-1) {

top++;

stack[top]=val;

cout<<val<<"inserted successfully"<<endl;

}


else {

cout<<"stack is overflow"<<endl;

}
}

int main() {

push(10);

push(2);

push(5);

push(8);

push(10);

push(28);



return 0;
}




output:10inserted successfully
2inserted successfully
5inserted successfully
8inserted successfully
10inserted successfully
stack is overflow
